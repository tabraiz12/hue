{
    "extends": [
        "prettier",
        "prettier/standard",
        "prettier/react",
        "plugin:react/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    },
    "env": {
        "browser": true
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "detect"
        }
    }
}